  <link name="back">
    <visual>
      <geometry>
        <box size="0.3 1.3 1"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.35"/>
      <material name="white">
        <color rgba="0.6 0.251 0.137 1"/>

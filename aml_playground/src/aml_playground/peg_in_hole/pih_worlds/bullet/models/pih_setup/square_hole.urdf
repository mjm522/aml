      <geometry>
        <box size="1 0.3 1"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 -0.15 0.35"/>
      <material name="white">
        <color rgba="0.6 0.251 0.137 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="1 0.3 1"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 -0.15 0.35"/>

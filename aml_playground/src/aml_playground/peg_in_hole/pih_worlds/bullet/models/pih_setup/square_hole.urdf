  <link name="table">
    <visual>
      <geometry>
        <box size="4 2 0.5"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 -0.25"/>
      <material name="white">
        <color rgba="0.6 0.251 0.137 1"/>
      </material>
    </visual>
    <collision>
      <geometry>

      <contact_cfm value="0.0"/>
      <contact_erp value="1.0"/>
    </contact>
  </link>

  <link name="top">
    <visual>
      <geometry>
        <box size="1 0.7 0.3"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.7"/>
      <material name="white">
        <color rgba="0.6 0.251 0.137 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="1 0.7 0.3"/>
